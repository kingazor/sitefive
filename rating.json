{
  "name": "Rating",
  "type": "object",
  "properties": {
    "server_id": {
      "type": "string",
      "description": "ID do servidor avaliado"
    },
    "user_id": {
      "type": "string",
      "description": "ID do usuário que avaliou"
    },
    "rating": {
      "type": "number",
      "minimum": 1,
      "maximum": 5,
      "description": "Nota de 1 a 5 estrelas"
    },
    "comment": {
      "type": "string",
      "description": "Comentário sobre o servidor"
    }
  },
  "required": [
    "server_id",
    "user_id",
    "rating"
  ]
}